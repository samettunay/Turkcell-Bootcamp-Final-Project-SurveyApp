{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "Chart.js@4.3.0",
      "destination": "wwwroot/Chart.js/",
      "files": [
        "chart.min.js"
      ]
    }
  ]
}